{{!< default}}

{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}
{{!-- Everything inside the #post tags pulls data from the post --}}
{{#post}}
  
  <article class="mv3 mv4-m mt5-l content">
    {{#if feature_image}}
    <figure class="mt0 nl3 nr3 nl4-l nr4-l">
      <img src="{{feature_image}}" alt="{{primary_tag.name}}" class="db">
    </figure>
    {{/if}}

    <h1 class="f2 f1-m lh-title mw7 normal">{{title}}</h1>

    {{content}}
  </article>

{{/post}}

{{> "book-consultation"}}
